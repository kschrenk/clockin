{
  "name": "clockin",
  "version": "1.0.0",
  "description": "A CLI app to track working hours and align them with paid hours",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "clockin": "./dist/bin/clockin.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/bin/clockin.ts",
    "start": "node dist/bin/clockin.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": ["cli", "time-tracking", "working-hours", "productivity"],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.18.3",
  "volta": {
    "node": "22.20.0"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "inquirer": "^9.2.12",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.3",
    "date-fns": "^2.30.0",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "open": "^10.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/inquirer": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0"
  }
}
