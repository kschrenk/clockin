{
  "name": "clockin",
  "version": "1.0.0",
  "description": "A CLI app to track working hours and align them with paid hours",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "clockin": "./dist/bin/clockin.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "NODE_ENV=development tsx src/bin/clockin.ts",
    "start": "node dist/bin/clockin.js",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "clean": "pnpm store prune"
  },
  "keywords": [
    "cli",
    "time-tracking",
    "working-hours",
    "productivity"
  ],
  "author": "kschrenk",
  "license": "ISC",
  "packageManager": "pnpm@10.18.3",
  "volta": {
    "node": "22.20.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.3",
    "commander": "^12.1.0",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^17.2.3",
    "inquirer": "^9.2.12",
    "open": "^10.1.0",
    "dayjs": "^1.11.13"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "8.0.3",
    "prettier": "3.2.5",
    "tsx": "^4.6.0",
    "typescript": "5.3.3",
    "vitest": "^1.0.0"
  }
}
